            <!-- category -->
            <header class="td-post-title">
                <h1 class="entry-title">Conoce estas 10 funciones de Excel y ejemplos</h1>
                

                <div class="td-module-meta-info">
                    <!-- author --><div class="td-post-author-name"><div class="td-author-by">Por</div> <a href="https://www.perucontable.com/excelcontable/author/perucontable/">Perucontable</a><div class="td-author-line"> - </div> </div>                    <!-- date --><span class="td-post-date"><time class="entry-date updated td-module-date" datetime="2024-08-28T10:46:00-05:00" >28 agosto 2024</time></span>                    <!-- comments --><div class="td-post-comments"><a href="https://www.perucontable.com/excelcontable/conoce-estas-10-funciones-deexcel-y-ejemplos/#respond"><i class="td-icon-comments"></i>0</a></div>                    <!-- views --><div class="td-post-views"><i class="td-icon-views"></i><span class="td-nr-views-2097">16811</span></div>                </div>

            </header>

        </div>

        <div class="td-post-sharing-top"><div id="td_social_sharing_article_top" class="td-post-sharing td-ps-bg td-ps-notext td-post-sharing-style1 ">
		<style>
